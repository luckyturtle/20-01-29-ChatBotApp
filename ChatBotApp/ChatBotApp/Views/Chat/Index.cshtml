/**
 * request getting right chattingroom body all messages from server.
 * param page
 * param lastLogId
 */
function displayRoom(page, lastLogId) {
    form_data = new FormData();
    form_data.append('agent', current_agent); //current selected agent or customer
    form_data.append('flag', current_type);   //agent:0 , customer: 1
    form_data.append('page', page);           //current page number
    form_data.append('pageSize', 10);         //pagesize=10 as default
    form_data.append('lastLogId', lastLogId); //last message id when getting data from server
    if (page == 1) $(".msg_history").html("");
    $.ajax({
        url: '@Url.Action("GetChatRoomLogData", "Chat")',
        data: form_data,
        cache: false,
        contentType: false,
        processData: false,
        type: 'POST',
        dataType: "json",
        success: function (response) {
            if (response.count > 0) {
                var html = "";
                var data = JSON.parse(response.data);
                for (var i = 0; i < data.length; i++) {
                    if (!current_type) {//if selected user is agent
                        if (data[i].chattingLog.Source == 2) {
                            html += getChatLogMessage(data[i].chattingLog.Id + ":1", 1, data[i].chattingLog.Text, data[i].chattingLog.Filename, getNowTime(getDateObj(data[i].chattingLog.Time)) + " | " + data[i].CustomerName);
                            //html += getChatLogMessage(data[i].chattingLog.Id + ":0", 0, data[i].CustomerName, "", "");
                        } else if (data[i].chattingLog.Source == 1) {
                            html += getChatLogMessage(data[i].chattingLog.Id + ":0", 0, data[i].chattingLog.Text, data[i].chattingLog.Filename, getNowTime(getDateObj(data[i].chattingLog.Time)) + " | " + data[i].CustomerName);
                            //html += getChatLogMessage(data[i].chattingLog.Id + ":0", 0, data[i].CustomerName, "", "");
                        }
                    } else {//if selected user is customer
                        if (data[i].chattingLog.Source == 0)//from bot
                            html += getChatLogMessage(data[i].chattingLog.Id + ":0", 0, data[i].chattingLog.Text, data[i].chattingLog.Filename, getNowTime(getDateObj(data[i].chattingLog.Time)) + " | Bot");
                        else if (data[i].chattingLog.Source == 1) {//from customer
                            html += getChatLogMessage(data[i].chattingLog.Id + ":1", 1, data[i].chattingLog.Text, data[i].chattingLog.Filename, getNowTime(getDateObj(data[i].chattingLog.Time)));
                        } else if (data[i].chattingLog.Source == 2) {//from agent
                            html += getChatLogMessage(data[i].chattingLog.Id + ":0", 0, data[i].chattingLog.Text, data[i].chattingLog.Filename, getNowTime(getDateObj(data[i].chattingLog.Time)) + " | " + data[i].AgentName);
                        }
                    }
                }
                //alert(html);
                html = html + $(".msg_history").html();
                $(".msg_history").html(html);
                if (AgentThreading == current_agent) setTimeout(function () {
                    displayRoom(page + 1, response.lastLogId);
                    //$(".msg_history").animate({ scrollTop: $(".msg_history").prop("scrollHeight") });//, "slow");
                }, 500);
            } else {
                $(".msg_history").animate({ scrollTop: $(".msg_history").prop("scrollHeight") }, "slow");
            }
        },
        error: function (xhr, textStatus, errorThrown) {

        }
    });
}

/**
 * logout action
 **/
function logoutAction() {
    $.ajax({
        url: '@Url.Action("Logout", "Account")',//" Url.Content("~")/Home/LoadSupplierData"
        type: 'POST',
        dataType: "json"
    });
}
/*
 *Saving a record into server, When logined agent send a message to any customer.
 */
function pingAction() {
    if (!current_type) return;
    if ($("#message-ping-obj").val() == "") return;
    var queryDate = new Date();


    var file_data = $('#attached_file').prop('files')[0];
    form_data = new FormData();
    form_data.append('queryTo', current_agent);                   //current selected customer
    form_data.append('queryDate', getValitDateFormat(queryDate)); //date
    form_data.append('queryText', $("#message-ping-obj").val());  //message text
    form_data.append('attachedFile', file_data);                  //attached file object
    $.ajax({
        url: '@Url.Action("ChattingEvent", "Chat")',
        cache: false,
        contentType: false,
        processData: false,
        data: form_data,
        type: 'POST',
        dataType: "json",
        success: function (response) {
            $('#attached_file').val('');
            if (response != null) {
                addPingAction(0, $("#message-ping-obj").val(), response.queryAth, queryDate);
                closeAttachedFileAction();
                $("#message-ping-obj").val("");
                $("#message-ping-obj").focus();
            }
        },
        error: function (xhr, textStatus, errorThrown) {

        }
    });
}
